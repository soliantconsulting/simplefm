{
    "name": "soliantconsulting/simplefm",
    "type": "library",
    "description": "FileMaker Server XML API Adapter",
    "keywords": [
        "FileMaker",
        "adapter"
    ],
    "homepage": "http://github.com/soliantconsulting/SimpleFM",
    "license": "MIT",
    "authors": [
        {"name": "Jeremiah Small", "email": "jsmall@soliantconsulting.com"}
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "doctrine/common": "^2.2.0",
        "zendframework/zend-crypt": "^3.1",
        "php-http/httplug": "^1.1",
        "litipk/php-bignumbers": "^0.7.3",
        "beberlei/assert": "^2.6"
    },
    "require-dev": {
        "zendframework/zend-debug": "2.5.*",
        "phpunit/PHPUnit": "^4.7",
        "phpunit/phpunit-skeleton-generator": "^2.0",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^2.6",
        "codeclimate/php-test-reporter": "^0.3"
    },
    "suggest": {
        "zendframework/zendframework": "Required if you plan on using SimpleFM/ZF2 components.",
        "ext-openssl": "Required for SimpleFM/ZF2/Authentication if you plan on enabling password retrieval."
    },
    "autoload": {
        "psr-4": {
            "Soliant\\SimpleFM\\": "src/",
            "SoliantTest\\SimpleFM\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
